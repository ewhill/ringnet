{
  "name": "ringnet",
  "version": "2.0.2",
  "homepage": "https://github.com/ewhill/ringnet",
  "description": "A secure peer-to-peer networking NodeJS module based on WebSockets using RSA and AES.",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc tape 'test/**/test*.js'",
    "coverage-html": "nyc --reporter=html tape 'test/**/test*.js'",
    "test": "tape 'test/test*.js' | faucet",
    "setup": "openssl genrsa -out https.key.pem 2048 && openssl req -new -key https.key.pem -out https.csr.pem && openssl x509 -req -days 9999 -in https.csr.pem -signkey https.key.pem -out https.cert.pem && node examples/peerSetup.js -o=first -b=2048 && node examples/peerSetup.js -o=second -b=2048 -ring=.ring.pem && node examples/peerSetup.js -o=third -b=2048 -ring=.ring.pem",
    "peer1": "node --no-warnings --no-deprecation examples/peerCommandLine.js -port=26781 -ring=.ring.pub -signature=first.peer.signature -private=first.peer.pem -public=first.peer.pub -publicAddress=127.0.0.1:26781",
    "peer2": "node --no-warnings --no-deprecation examples/peerCommandLine.js -port=26782 -peers=127.0.0.1:26781 -ring=.ring.pub -signature=second.peer.signature -private=second.peer.pem -public=second.peer.pub -publicAddress=127.0.0.1:26782",
    "peer3": "node --no-warnings --no-deprecation examples/peerCommandLine.js -port=26783 -peers=127.0.0.1:26781 -ring=.ring.pub -signature=third.peer.signature -private=third.peer.pem -public=third.peer.pub -v -publicAddress=127.0.0.1:26783",
    "worker1": "node --no-warnings --no-deprecation examples/peerCoordination/index.js -port=26781 -private=first.peer.pem -public=first.peer.pub -signature=first.peer.signature -ring=.ring.pem -publicAddress=127.0.0.1:26781/worker -console",
    "worker2": "node --no-warnings --no-deprecation examples/peerCoordination/index.js -port=26782 -peers=127.0.0.1:26781/worker -private=second.peer.pem -public=second.peer.pub -signature=second.peer.signature -ring=.ring.pem -publicAddress=127.0.0.1:26782/worker",
    "worker3": "node --no-warnings --no-deprecation examples/peerCoordination/index.js -port=26783 -peers=127.0.0.1:26781/worker -private=third.peer.pem -public=third.peer.pub -signature=third.peer.signature -ring=.ring.pem -publicAddress=127.0.0.1:26783/worker"
  },
  "keywords": [
    "peer-to-peer",
    "p2p",
    "discovery",
    "trust",
    "network",
    "networking",
    "http",
    "https",
    "secure",
    "rsa",
    "aes",
    "aes-256-cbc"
  ],
  "author": "Eric Hill",
  "repository": {
    "type": "git",
    "url": "https://github.com/ewhill/ringnet"
  },
  "license": "MIT",
  "dependencies": {
    "ws": "^6.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "aliasify": "^2.1.0",
    "babelify": "^10.0.0",
    "browserify-shim": "^3.8.14",
    "crypto-browserify": "^3.12.0",
    "faucet": "0.0.1",
    "handlebars": "^4.5.3",
    "nyc": "^15.1.0",
    "tape": "^4.13.3",
    "tinyify": "^3.0.0"
  }
}
